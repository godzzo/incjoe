{
	"srv": {
		"docRoot": "./sakila/www",
		"port": 8822,
		"appRoot": "./sakila/app",
		"mode": "POST_GEN"
	},
	"db": {
        "host": "localhost",
        "port": 3306,
        "database": "sakila",
        "user": "user",
        "password": "pwd"
    },
    "templateEngine": "none",
    "invokable": [
        {
            "name": "actors",
            "mask": ".*actors.*",
			"sql": "${obj.config.srv.appRoot}/sql/actors.sql",
			"contentType": "object",
			"notStringify": true,
            "module": "./util_ijoe_mysql.js",
            "action": "RunSql"
        }
    ]
}